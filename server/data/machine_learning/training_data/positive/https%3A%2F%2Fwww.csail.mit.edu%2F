<!-- CSAIL theme customized twitter feed  --> 
<script>                                                                           
var hideTwitterAttempts = 0;                                                                           
function hideTwitterBoxElements() {                                                                    
    setTimeout( function() {                                                                           
        var list = document.getElementsByTagName('iframe');                                            
        if (list.length ) {                                                                            
            Array.prototype.forEach.call(                                                              
                list,                                                                                  
                function(element){                                                                     
                    var footerE = element.contentDocument.getElementsByClassName('timeline-footer')[0];
                    footerE.style.backgroundColor="transparent";
		    var aTitleLink = element.contentDocument.getElementsByClassName('customisable-highlight')[0];
                    aTitleLink.style.textTransform="uppercase";
		    var aTitleLinkColor = element.contentDocument.getElementsByClassName('customisable-highlight')[0];
                    aTitleLinkColor.style.color="#3c636d";
		    var aTitleLinksize = element.contentDocument.getElementsByClassName('customisable-highlight')[0];
                    aTitleLinksize.style.fontSize="11px";
		    var aTextsize = element.contentDocument.getElementsByClassName('e-entry-title')[0];
                    aTextsize.style.fontSize="11px";
                });                                                                                    
        }                                                                                              
        hideTwitterAttempts++;                                                                         
        if ( hideTwitterAttempts < 3 ) {                                                               
            hideTwitterBoxElements();                                                                  
        }                                                                                              
    }, 1500);                                                                                          
}                                                                                                      
hideTwitterBoxElements();                                                                              
</script> <!-- end twitter feed  --> 

